  "scripts": {
    "prebuild": "rimraf dist && rimraf .next",
    "build": "yarn build:nest && yarn build:next",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:nest": "cross-env NODE_ENV=production nest build --path ./tsconfig.server.json",
    "start:dev": "cross-env NODE_ENV=development nest start --path ./tsconfig.server.json --watch",
    "start:prod": "yarn prebuild && yarn build && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "sb": "start-storybook -p 6006 --no-open",
    "build-sb": "build-storybook"
  },
