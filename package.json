{
  "name": "nest-next-project",
  "version": "0.0.1",
  "description": "",
  "author": "<TheBaconStyle@yandex.ru>TheBaconStyle",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:nest && yarn build:next",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:nest": "cross-env NODE_ENV=production nest build --path ./tsconfig.server.json",
    "start:dev": "cross-env NODE_ENV=development next build && cross-env NODE_ENV=development nest start --path ./tsconfig.server.json --watch",
    "start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
    "start:prod": "yarn prebuild && yarn build && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/bcrypt": "^5.0.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "colors": "^1.4.0",
    "dayjs": "^1.11.1",
    "express-session": "^1.17.2",
    "nest-next": "^10.0.0",
    "next": "^12.1.6",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.0.7",
    "typeorm": "0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "postcss": "^8.4.13",
    "prettier": "^2.3.2",
    "sass": "^1.51.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
