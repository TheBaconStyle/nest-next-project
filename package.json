{
  "name": "nest-next",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && rimraf .next && rimraf database && rimraf migrations",
    "build": "yarn build:next && yarn build:nest && yarn migrations:generate && yarn migrations:run",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "component": "generate-react component --type=component",
    "page": "generate-react component --type=page",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "yarn typeorm migration:generate ./migrations/migration -d ./ormconfig.ts",
    "migrations:run": "yarn typeorm migration:run -d ./ormconfig.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^9.0.0",
    "@reduxjs/toolkit": "^1.8.4",
    "@shwao/express-fingerprint": "^2.0.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "classnames": "^2.3.1",
    "colors": "^1.4.0",
    "connect-typeorm": "^2.0.0",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.4",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "express-session": "^1.17.3",
    "framer-motion": "^7.1.0",
    "global": "^4.4.0",
    "helmet": "^5.1.1",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nest-next": "^10.0.0",
    "next": "^12.2.4",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.0.11",
    "swagger-ui-express": "^4.5.0",
    "swr": "^1.3.0",
    "typeorm": "^0.3.9",
    "usehooks-ts": "^2.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/colors": "^1.2.1",
    "@types/csurf": "^1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1",
    "@types/jest": "28.1.4",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1",
    "@types/node": "^16.0.0",
    "@types/passport": "^0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/typescript-estree": "^5.38.1",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "generate-react-cli": "^7.3.0",
    "jest": "28.1.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tailwindcss": "^3.1.8",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "<4.8",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.2.2"
}
