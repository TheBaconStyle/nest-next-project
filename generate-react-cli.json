{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "component": {
    "component": {
      "customTemplates": {
        "index": "templates/component/index.ts",
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss"
      },
      "path": "src/client/components",
      "withIndex": true,
      "withStyle": true
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/index.tsx",
        "style": "templates/page/TemplateName.module.scss"
      },
      "path": "src/pages/views",
      "withStyle": true
    },
    "default": {
      "path": "src/client/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  },
  "testLibrary": "None"
}
